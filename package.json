{
  "name": "cqfill",
  "version": "0.1.1",
  "description": "Polyfill for CSS Container Queries",
  "type": "module",
  "main": "dist/cqfill.cjs",
  "module": "dist/cqfill.mjs",
  "typings": "dist/cqfill.d.ts",
  "jsdelivr": "dist/cqfill.js",
  "unpkg": "dist/cqfill.js",
  "exports": {
    "require": "./dist/cqfill.cjs",
    "import": "./dist/cqfill.mjs",
    "default": "./dist/cqfill.mjs"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "jsxtools/cqfill",
  "homepage": "https://github.com/jsxtools/cqfill#readme",
  "bugs": "https://github.com/jsxtools/cqfill/issues",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "node .bin/build"
  },
  "devDependencies": {
    "esbuild": "^0.11.14",
    "nodemon": "^2.0.7",
    "terser": "^5.6.1"
  },
  "keywords": [
    "container",
    "css",
    "csswg",
    "element",
    "features",
    "future",
    "javascript",
    "js",
    "layout",
    "polyfill",
    "query",
    "queries",
    "specifications",
    "specs",
    "styles"
  ]
}
